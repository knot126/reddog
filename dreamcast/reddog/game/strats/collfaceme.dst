STATE Init 
	MyFlag = MyFlag | STRATCOLL | FLOORCOLL | ENEMY | EXPLODEPIECE | BULLETTYPE
	OBJECT> P:\GAME\OBJECTS\MISC\COLLFACE
	TRIGSET>KillOff	WHENHIT
	TRIGSET>KillOff	ONFLOOR
  	SetFriction(0.0, 0.0, 0.0)
  	MoveY(2.0)
	STAT> Start
ENDSTATE

STATE Start
	StickToParent(0.0, 0.0, 0.0)
ENDSTATE

STATE KillOff
	CREATE SPAWN_BLASTEXP 0, 0, 0, 0, 0, 0, 0
	Delete()
ENDSTATE